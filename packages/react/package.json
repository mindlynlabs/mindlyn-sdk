{
  "name": "@mindlyn/react",
  "version": "0.0.3",
  "private": false,
  "description": "React components for Mindlyn SDK",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "mindlyn",
    "react",
    "components",
    "sdk",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mindlynlabs/mindlyn-sdk.git",
    "directory": "packages/react"
  },
  "bugs": {
    "url": "https://github.com/mindlynlabs/mindlyn-sdk/issues"
  },
  "homepage": "https://github.com/mindlynlabs/mindlyn-sdk/tree/main/packages/react#readme",
  "engines": {
    "node": ">=18.17"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "LICENSE"
  ],
  "sideEffects": false,
  "packageManager": "pnpm@10.10.0",
  "scripts": {
    "dev": "NODE_ENV=development tsup",
    "build:css": "postcss src/styles.css -o dist/styles.css --env production",
    "build": "pnpm types && NODE_ENV=production tsup",
    "types": "tsc -b",
    "lint": "eslint .",
    "lint:fix": "pnpm lint --fix",
    "prepublishOnly": "pnpm lint && pnpm types && pnpm build"
  },
  "peerDependencies": {
    "@radix-ui/react-dialog": "^1",
    "@radix-ui/react-dropdown-menu": "^2",
    "react": ">=18 <=19",
    "react-dom": ">=18 <=19",
    "react-markdown": "^10"
  },
  "dependencies": {
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "nanoid": "^5.1.5",
    "remark-breaks": "^4.0.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.2.0"
  },
  "devDependencies": {
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@tailwindcss/postcss": "^4.1.6",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.3",
    "postcss": "^8.5.3",
    "postcss-cli": "^11.0.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-markdown": "^10.1.0",
    "tailwindcss": "^4.1.6",
    "tsup": "^8.4.0",
    "tw-animate-css": "^1.2.9"
  }
}
